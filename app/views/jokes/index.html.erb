This is the Jokes Page!

<%= form_for [@joke] do |f| %>
  <%= f.label :text %>
  <%= f.text_field :text %>
  <%= f.submit %>
<% end %>
<br>

<% if @jokes %>
  
  <% @jokes.each do |joke| %>

    <%= joke.text %>
    <%= joke.rating %>
    <%= link_to "Delete", joke_path(joke), method: :delete %>
    <br>
    <br>

  <% end %>

<% end %>

<% @users.each do |user| %>
  
  <%= user.email %>
  <%= user.rating %>
  <br>

<% end %>